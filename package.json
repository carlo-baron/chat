{
  "name": "chat",
  "version": "1.0.0",
  "description": "Websocket practice",
  "main": "index.js",
  "scripts": {
    "init": "concurrently \"npm run initClient\" \"npm run initServer\"",
    "initClient": "cd client && npm install",
    "initServer": "cd server && npm install",
    "client": "cd client && npm run dev",
    "server": "cd server && nodemon index.js",
    "dev": "concurrently \" npm run client\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlo-baron/chat.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carlo-baron/chat/issues"
  },
  "homepage": "https://github.com/carlo-baron/chat#readme",
  "dependencies": {
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "mongoose": "^8.17.2",
    "nodemon": "^3.1.10"
  }
}
